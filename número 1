#include<stdio.h>
#include<math.h>
main(){

  float funcao, xis;

  printf("Observe a funcao: 5*x+3/raiz(x*x-16)");
  
  printf("\nDigite um valor para representar x");
    scanf("%f", &xis);

  funcao = (5*xis+3)/sqrt(xis*xis-16);

  printf("\nO resultado da funcao, com o valor apresentado para x, e igual a %f", funcao);

}
